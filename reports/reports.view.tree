$mpk_tss_reports $mpk_tss_wrap
	self_title @ \Reports
	report_current_page null
	sub /
		<= Head
		<= Filter_title $mol_search
			query?val <=> filter_title?val \
		<= Body
	body /
		<= Reports $mol_list
			destructor <= destructor
			rows <= report_links /
	-
	Report_link!id $mpk_tss_reports_card
		report <= report!id null
	-
	Report_details!id $mpk_tss_reports_details
		minimal_width 400
		report <= report!id null
