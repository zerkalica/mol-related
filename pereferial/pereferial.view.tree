$mpk_tss_pereferial $mpk_tss_wrap
	self_title @ \Pereferials
	minimal_width 200
	sub /
		<= Head
		<= Filter_title $mol_search
			query?val <=> filter_title?val \
		<= Body
	body /
		<= Units $mol_list
			destructor <= destructor
			rows <= unit_links /
	-
	Unit_link!id $mpk_tss_pereferial_card
		unit <= unit!id null
	-
	Unit_details!id $mpk_tss_pereferial_details
		minimal_width 200
		unit <= unit!id null
	